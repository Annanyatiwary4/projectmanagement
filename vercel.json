{
    "version": 2,
    "builds": [
        {
            "src": "projectfolio/wsgi.py",
            "use": "@vercel/python",
            "config": {
            "runtime": "python3.12.6"
      }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/media/(.*)",
            "dest": "/media/$1"
        },
        {
            "src": "/(.*)",
            "dest": "projectfolio/wsgi.py"
        }
    ]
}